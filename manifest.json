{
  "name": "SoundBox",
  "version": "1.1",
  "description": "Ejemplo de extension de Google Chrome que muestra tus datos en Foros del Web.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "SoundBox",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [ "popup.js" ]
  },
  "manifest_version": 2
}